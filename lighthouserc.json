{
	"ci": {
		"collect": {
			"staticDistDir": "./build",
			"numberOfRuns": 3,
			"maxAutodiscoverUrls": 20
		},
		"assert": {
			"preset": "lighthouse:no-pwa",
			"assertions": {
				"uses-passive-event-listeners": "warn",
				"csp-xss": "warn"
			}
		},
		"upload": {
			"target": "temporary-public-storage"
		}
	}
}
