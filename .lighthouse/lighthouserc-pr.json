{
	"ci": {
		"collect": {
			"url": ["http://localhost/tools/breath", "http://localhost/games/minesweeper"],
			"staticDistDir": "./build"
		},
		"assert": {
			"preset": "lighthouse:no-pwa",
			"assertions": {
				"uses-passive-event-listeners": "warn",
				"csp-xss": "warn"
			}
		},
		"upload": {
			"target": "temporary-public-storage"
		}
	}
}
